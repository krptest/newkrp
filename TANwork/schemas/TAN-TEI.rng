              <ref name="editor"/>
              <ref name="respStmt"/>
            </choice>
          </zeroOrMore>
          <zeroOrMore>
            <choice>
              <ref name="idno"/>
              <ref name="biblScope"/>
            </choice>
          </zeroOrMore>
        </group>
      </choice>
      <ref name="att.global.attributes"/>
      <empty/>
    </element>
