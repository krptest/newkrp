  <define name="div6">
    <element name="tei:div6">
      <a:documentation>(level-6 text division) contains a sixth-level subdivision of the front, body, or back of a text. [4.1.2. Numbered Divisions]</a:documentation>
      <group>
        <zeroOrMore>
          <choice>
            <ref name="model.divTop"/>
            <ref name="model.global"/>
          </choice>
        </zeroOrMore>
        <optional>
          <choice>
            <oneOrMore>
              <choice>
                <ref name="model.div7Like"/>
                <ref name="model.divGenLike"/>
              </choice>
