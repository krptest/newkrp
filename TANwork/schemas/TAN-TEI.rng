      <ref name="att.global.attributes"/>
      <ref name="att.datable.attributes"/>
      <ref name="att.typed.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="surrogates">
    <element name="tei:surrogates">
      <a:documentation>contains information about any representations of the manuscript being described which may exist in the holding institution or elsewhere. [10.9. Additional Information]</a:documentation>
      <ref name="macro.specialPara"/>
      <ref name="att.global.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="msPart">
