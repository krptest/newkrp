      <choice>
        <group>
          <group>
            <ref name="titleStmt"/>
            <optional>
              <ref name="editionStmt"/>
            </optional>
            <optional>
              <ref name="extent"/>
            </optional>
            <ref name="publicationStmt"/>
            <optional>
              <ref name="seriesStmt"/>
            </optional>
            <optional>
              <ref name="notesStmt"/>
            </optional>
          </group>
          <zeroOrMore>
